{
  "shopee": {
    "domain": "shopee.tw",
    "name": "蝦皮購物",
    "selectors": {
      "name": "._2rQP1z, .product-title",
      "price": ".pqTWkA, ._3n5NQx, .product-price",
      "image": ".product-image img, ._2JKBEh img"
    },
    "urlPattern": "https://shopee.tw/"
  },
  "momo": {
    "domain": "momoshop.com.tw",
    "name": "momo購物網",
    "selectors": {
      "name": ".prdName, .prodTitle h3, #goods_name",
